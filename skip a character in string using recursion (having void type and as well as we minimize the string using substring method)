/*package whatever //do not write package name here */

import java.io.*;

class GFG {
	public static void main (String[] args) {
	
		String s="baccad";
	
		skip(s);
		
	}
	static String value=" ";
	public static void skip(String s){
	    if(s.length()==0){
	       System.out.println(value);
	       return;
	    }
	    if(s.charAt(0)!='a'){
	        value=value+s.charAt(0);
	    }
	    skip(s.substring(1)) ;
	}
}
